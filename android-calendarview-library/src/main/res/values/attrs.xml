<resources>
    <!-- Framework styleables are not public. -->
    <declare-styleable name="TextAppearanceCompatStyleable">
        <attr name="android:textSize"/>
    </declare-styleable>

    <declare-styleable name="calendar_cell">
        <attr name="state_selectable" format="boolean"/>
        <attr name="state_today" format="boolean"/>
        <attr name="state_range_start" format="boolean"/>
        <attr name="state_range_end" format="boolean"/>
        <attr name="state_range_middle" format="boolean"/>
        <attr name="state_pre_defined_range_start" format="boolean"/>
        <attr name="state_pre_defined_range_end" format="boolean"/>
        <attr name="state_pre_defined_range_middle" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="calendar_size">
        <attr name="prev_months" format="integer"/>
        <attr name="next_months" format="integer"/>
    </declare-styleable>

</resources>
